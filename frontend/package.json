{
  "name": "sward-drop-frontend-stencil",
  "private": true,
  "version": "0.0.1",
  "description": "Sward Drop Frontend",
  "scripts": {
    "build": "stencil build --prerender",
    "start": "stencil build --dev --watch --serve",
    "test": "jest",
    "test.watch": "jest --watch"
  },
  "dependencies": {
    "@stencil/core": "0.11.4",
    "@stencil/router": "0.2.5",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "jest": "^23.4.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/srmward/sward-drop"
  },
  "author": "Stephen Ward",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ionic-team/stencil"
  },
  "homepage": "https://sward-drop.co",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
